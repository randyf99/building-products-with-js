{
  "name": "experts-server",
  "version": "0.1.0",
  "description": "Experts REST backend",
  "main": "index.js",
  "scripts": {
    "start": "node index.js",
    "db:start": "docker run -d -p 28015:28015 -p 8090:8080 -v $PWD/db:/data --name expertsdb rethinkdb",
    "db:stop": "docker stop expertsdb",
    "db:rm": "docker rm expertsdb",
    "eslint": "eslint src/",
    "test": "eslint src/ && NODE_ENV=testing node test/index.js | tap-spec"
  },
  "keywords": [
    "rest",
    "javascript",
    "experts"
  ],
  "author": "Randy Ferrer",
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.14.0",
    "babel-core": "^6.14.0",
    "babel-eslint": "^6.1.2",
    "babel-preset-es2015-node": "^6.1.1",
    "babel-preset-stage-0": "^6.5.0",
    "eslint": "^3.4.0",
    "eslint-config-airbnb": "^10.0.1",
    "eslint-plugin-import": "^1.14.0",
    "eslint-plugin-jsx-a11y": "^2.2.0",
    "eslint-plugin-react": "^6.1.2",
    "reqlite": "^2.3.0",
    "supertest": "^2.0.0",
    "tap-spec": "^4.1.1",
    "tape": "^4.6.0"
  },
  "babel": {
    "presets": [
      "es2015-node",
      "stage-0"
    ]
  },
  "dependencies": {
    "body-parser": "^1.15.2",
    "cookie-parser": "^1.4.3",
    "express": "^4.14.0",
    "express-session": "^1.14.1",
    "jsonwebtoken": "^7.1.9",
    "moment": "^2.15.0",
    "morgan": "^1.7.0",
    "passport": "^0.3.2",
    "passport-jwt": "^2.1.0",
    "passport-local": "^1.0.0",
    "thinky": "^2.3.6",
    "winston": "^2.2.0"
  }
}
